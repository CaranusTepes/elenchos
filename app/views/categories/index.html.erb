<h1> LIST </h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Details</th>
    </tr>
  </thead>

  <tbody>
  
    <% @categories.each do |categories| %>
      <tr>
        <td><%= categories.title   %></td>
        <td><%= categories.details %></td>
      </tr>
    <% end %>
  </tbody>
</table>